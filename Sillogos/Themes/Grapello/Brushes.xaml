<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--BASE COLORS-->
    <!--<Color x:Key="BlackColor">#FF000000</Color>-->
    <Color x:Key="WhiteColor">#FFFFFFFF</Color>
    <Color x:Key="Gray1">#FFCCCCCC</Color>
    <!--<Color x:Key="Gray2">#FF7F7F7F</Color>-->
    <!--<Color x:Key="Gray3">#FF333333</Color>-->
    <!--<Color x:Key="Gray4">#FFB9B9B9</Color>-->
    <Color x:Key="Gray5">#FFD8D8D9</Color>

    <!--Used in BorderStyle.PAGE HEADER BORDER START-->
    <!--=============================================-->
    <!--<SolidColorBrush x:Key="Border.PageHeader.Background" Color="#FF595B87"/>-->
    <SolidColorBrush x:Key="Border.PageHeader.Background" Color="#FFF1F1F1"/>
    <!--Used in BorderStyle.PAGE HEADER BORDER END-->

    <!--Used in BorderStyle.CoreBorderStyle START-->
    <!--=============================================-->
    <!--<SolidColorBrush x:Key="ThemeColorBrush" Color="#2B579A"></SolidColorBrush>-->
    <SolidColorBrush x:Key="ThemeColorBrush" Color="#595B87"></SolidColorBrush>
    <!--Used in BorderStyle.CoreBorderStyle END-->

    <!--Used in ButtonStyle.App-Bar-Buttons START-->
    <!--=============================================-->
    <!--<SolidColorBrush x:Key="Button.Static.Background" Color="#FFDDDDDD"/>-->
    <SolidColorBrush x:Key="Button.Static.Background" Color="Transparent" />
    <!--<SolidColorBrush x:Key="Button.Static.Foreground" Color="#FF3E3E3E"/>-->
    <SolidColorBrush x:Key="Button.Static.Foreground" Color="#FF333333" />
    <SolidColorBrush x:Key="Button.Static.Border" Color="#FF707070" />
    <!--<SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFBEE6FD"/>-->
    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFDDDDDD" />
    <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF3C7FB1" />
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FFC4E5F6" />
    <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF2C628B" />
    <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4" />
    <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5" />
    <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383" />
    <!--Used in ButtonStyle.App-Bar-Buttons END-->

    <!--Used in ButtonStyle.SearchButton START-->
    <!--=============================================-->
    <Color x:Key="HighlightDarkColor">#FF119EDA</Color>
    <Color x:Key="HighlightLightColor">#FFB2E0F4</Color>
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#FFAAAAAA" />
    <SolidColorBrush x:Key="TextBoxBorderBrush" Color="{StaticResource Gray1}" />
    <!--Used in ButtonStyle.SearchButton END-->

    <!--Used in ComboBoxStyle.GridComboBox START-->
    <!--=============================================-->
    <SolidColorBrush x:Key="ComboBox.Disabled.Background" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Border" Color="#FFD9D9D9"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Editable.Background" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Editable.Border" Color="#FFBFBFBF"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Editable.Button.Background" Color="Transparent"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Editable.Button.Border" Color="Transparent"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Foreground" Color="#FF707070"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Glyph" Color="#FFBFBFBF"/>
    <LinearGradientBrush x:Key="ComboBox.Static.Background" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FFF0F0F0" Offset="0.0"/>
        <GradientStop Color="#FFE5E5E5" Offset="1.0"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ComboBox.Static.Border" Color="#FFACACAC"/>
    <SolidColorBrush x:Key="ComboBox.Static.Editable.Background" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="ComboBox.Static.Editable.Border" Color="#FFABADB3"/>
    <SolidColorBrush x:Key="ComboBox.Static.Editable.Button.Background" Color="Transparent"/>
    <SolidColorBrush x:Key="ComboBox.Static.Editable.Button.Border" Color="Transparent"/>
    <SolidColorBrush x:Key="ComboBox.Static.Glyph" Color="#FF606060"/>
    <LinearGradientBrush x:Key="ComboBox.MouseOver.Background" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FFECF4FC" Offset="0.0"/>
        <GradientStop Color="#FFDCECFC" Offset="1.0"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Border" Color="#FF7EB4EA"/>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Editable.Background" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Editable.Border" Color="#FF7EB4EA"/>
    <LinearGradientBrush x:Key="ComboBox.MouseOver.Editable.Button.Background" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FFEBF4FC" Offset="0.0"/>
        <GradientStop Color="#FFDCECFC" Offset="1.0"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Editable.Button.Border" Color="#FF7EB4EA"/>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Glyph" Color="#FF000000"/>
    <LinearGradientBrush x:Key="ComboBox.Pressed.Background" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FFDAECFC" Offset="0.0"/>
        <GradientStop Color="#FFC4E0FC" Offset="1.0"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ComboBox.Pressed.Border" Color="#FF569DE5"/>
    <SolidColorBrush x:Key="ComboBox.Pressed.Editable.Background" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="ComboBox.Pressed.Editable.Border" Color="#FF569DE5"/>
    <LinearGradientBrush x:Key="ComboBox.Pressed.Editable.Button.Background" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FFDAEBFC" Offset="0.0"/>
        <GradientStop Color="#FFC4E0FC" Offset="1.0"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ComboBox.Pressed.Editable.Button.Border" Color="#FF569DE5"/>
    <SolidColorBrush x:Key="ComboBox.Pressed.Glyph" Color="#FF000000"/>
    <!--Used in ComboBoxStyle.GridComboBox END-->

    <!--Used in LabelStyle.PageHeaderLabel START-->
    <!--=============================================-->
    <!--<SolidColorBrush x:Key="Label.PageHeader.Foreground" Color="#FFF5F5F5"/>-->
    <SolidColorBrush x:Key="Label.PageHeader.Foreground" Color="#FF333333"/>
    <!--Used in LabelStyle.PageHeaderLabel END-->

    <!--Used in LabelStyle.EditPageHeaderLabel START-->
    <!--=============================================-->
    <SolidColorBrush x:Key="Label.EditPageHeader.Foreground" Color="#FFF5F5F5"/>
    <!--Used in LabelStyle.EditPageHeaderLabel END-->

    <!--Used in DataGridStyle START-->
    <!--=============================================-->
    <SolidColorBrush x:Key="DataGrid.BorderBrush" Color="#FF688CAF"/>
    <SolidColorBrush x:Key="DataGrid.GridLines" Color="#FFDDDDDD"/>
    <SolidColorBrush x:Key="DataGrid.MouseOver.Background" Color="#FFF2F2F2"/>
    <!-- Από το ButtonStyle <SolidColorBrush x:Key="Button.Static.Foreground" Color="#FF333333"/>-->
    <!--Used in DataGridStyle END-->

    <!--Used in MenuStyle.CoreMenuStyle START-->
    <!--=============================================-->
    <!--<SolidColorBrush x:Key="Menu.Static.Background" Color="Transparent"/>-->
    <SolidColorBrush x:Key="Menu.Static.Background" Color="#FF595B87"/>
    <SolidColorBrush x:Key="Menu.Static.Foreground" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="Menu.Static.Foreground2" Color="#FF212121"/>
    <!--<SolidColorBrush x:Key="Menu.Static.Background2" Color="#FFF0F0F0"/>-->
    <!--<SolidColorBrush x:Key="Menu.Static.Border" Color="#FF999999"/>-->
    <!--Used in MenuStyle.CoreMenuStyle END-->

    <!--Used in TextBoxStyle.GridTextBox START-->
    <!--=============================================-->
    <SolidColorBrush x:Key="TextBox.Static.Border" Color="#FFABAdB3"/>
    <SolidColorBrush x:Key="TextBox.MouseOver.Border" Color="#FF7EB4EA"/>
    <SolidColorBrush x:Key="TextBox.Focus.Border" Color="#FF569DE5"/>
    <!--Used in TextBoxStyle.GridTextBox END-->

</ResourceDictionary>